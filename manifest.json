{
    "name": "WNDWJ",
    "version": "0.3",
    "manifest_version": 2,
    "description": "WNDWJ",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },

    "background": {
        "page": "background.html"
    },
    "options_page": "options.html",

    "page_action": {
        "default_icon": {
            "19": "icons/19.png",
            "38": "icons/38.png"
        },
        "default_title": "WNDWJ"
    },
    "content_scripts": [{
        "matches": ["http://forum.xitek.com/thread*.html"],
        "js": ["jquery-1.9.1.min.js","main.js"],
        "run_at": "document_idle"
    }]

}
