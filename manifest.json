{
	"name": "WNDWJ",
	"version": "0.2",
	"manifest_version": 2,
	"description": "自定义过滤器",
	"background": {
		"page" : "background.html"
	},
	"page_action":
	{
		"default_icon": {
			"19": "icons/19.png",
			"38": "icons/38.png"
		},
		"default_title": "WNDWJ"
	},
	"content_scripts": [
	{
		"matches": ["http://forum.xitek.com/thread*.html"],
		"js": ["js/jquery-1.8.3.min.js","js/main.js"],
		"run_at": "document_idle"
	}
	],
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"options_page" : "options.html"
}
